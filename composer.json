{
    "name": "cove/macro-attributes",
    "description": "Use attributes to add macros and mixins to any macroable class in Laravel.",
    "license": "MIT",
    "keywords": [
        "laravel",
        "mixins",
        "macroable",
        "macro",
        "attributes"
    ],
    "authors": [
        {
            "name": "Sven Luijten",
            "email": "contact@svenluijten.com",
            "homepage": "https://svenluijten.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "haydenpierce/class-finder": "^0.4.3",
        "illuminate/macroable": "^10.0",
        "illuminate/support": "^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.18",
        "phpunit/phpunit": "^10"
    },
    "autoload": {
        "psr-4": {
            "Cove\\MacroAttributes\\": "src/"
        }
    },
    "autoload-dev": {
        "files": [
            "tests/Cases/bootstrap.php"
        ],
        "psr-4": {
            "Cove\\MacroAttributes\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cove\\MacroAttributes\\ServiceProvider"
            ]
        }
    }
}
